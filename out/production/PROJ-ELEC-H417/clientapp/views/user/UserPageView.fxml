<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="480.0" prefWidth="780.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientapp.views.user.UserPageViewController">
    <Button fx:id="logOutButton" layoutX="535.0" layoutY="435.0" mnemonicParsing="false" onAction="#onLogOutButton" prefHeight="25.0" prefWidth="150.0" text="Log Out" />
    <Label layoutX="549.0" layoutY="24.0" style="-fx-background-color: white;" text="META">
        <font>
            <Font size="30.0" />
        </font>
    </Label>
    <VBox layoutX="10.0" layoutY="10.0" prefHeight="450.0" prefWidth="430.0">
        <children>
            <TextArea fx:id="readingArea" editable="false" prefHeight="426.0" prefWidth="454.0" wrapText="true" />
            <HBox prefHeight="10.0" prefWidth="415.0">
                <children>
                    <TextField fx:id="writtingField" prefHeight="26.0" prefWidth="380.0" promptText="say hello" />
                    <Button mnemonicParsing="false" onAction="#onSendButton" prefHeight="26.0" text="Send" />
                </children>
            </HBox>
        </children>
    </VBox>
    <ImageView fitHeight="79.0" fitWidth="140.0" layoutX="655.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
        <image>
            <Image url="@meta.png" />
        </image>
    </ImageView>
   <TextField fx:id="destinationTextField" layoutX="525.0" layoutY="210.0" prefHeight="30.0" prefWidth="170.0" promptText="Send to ..." />
    <Label fx:id="errorMessageLabel" alignment="TOP_CENTER" contentDisplay="TOP" layoutX="440.0" layoutY="250.0" prefHeight="100.0" prefWidth="340.0" text="errorMessage" textAlignment="CENTER" textFill="RED" visible="false" wrapText="true" />
   <Label fx:id="identificationLabel" alignment="TOP_CENTER" contentDisplay="CENTER" layoutX="450.0" layoutY="80.0" prefHeight="130.0" prefWidth="300.0" text="IdentificationLabel" textAlignment="CENTER" wrapText="true">
      <font>
         <Font size="20.0" />
      </font></Label>
</AnchorPane>
