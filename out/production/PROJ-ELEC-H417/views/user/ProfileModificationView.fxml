<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.user.ProfileModificationViewController">
    <Label layoutX="235.0" layoutY="65.0" prefHeight="17.0" prefWidth="150.0" text="First name" />
    <Label layoutX="235.0" layoutY="120.0" prefHeight="17.0" prefWidth="150.0" text="Last name" />
    <Label layoutX="235.0" layoutY="175.0" prefHeight="17.0" prefWidth="150.0" text="Username" />
    <Label layoutX="235.0" layoutY="230.0" prefHeight="17.0" prefWidth="150.0" text="Email address" />
    <Label layoutX="235.0" layoutY="285.0" prefHeight="18.0" prefWidth="150.0" text="Old password" />
    <Label layoutX="235.0" layoutY="340.0" prefHeight="17.0" prefWidth="150.0" text="New password" />
    <TextField fx:id="firstnameTextField" layoutX="235.0" layoutY="85.0" prefHeight="26.0" prefWidth="250.0" />
    <TextField fx:id="lastnameTextField" layoutX="235.0" layoutY="140.0" prefHeight="26.0" prefWidth="250.0" />
    <TextField fx:id="usernameTextField" disable="true" layoutX="235.0" layoutY="195.0" prefHeight="26.0" prefWidth="250.0" />
    <TextField fx:id="emailTextField" layoutX="235.0" layoutY="250.0" prefHeight="26.0" prefWidth="250.0" />
    <PasswordField fx:id="oldPasswordField" layoutX="235.0" layoutY="303.0" prefHeight="26.0" prefWidth="250.0" promptText="Enter old password" />
    <PasswordField fx:id="newPasswordField" layoutX="235.0" layoutY="358.0" prefHeight="26.0" prefWidth="250.0" promptText="Enter new password" />
    <Button fx:id="confirmModificationsButton" layoutX="235.0" layoutY="413.0" mnemonicParsing="false" onAction="#onConfirmModificationsButton" prefHeight="42.0" prefWidth="250.0" text="Confirm modifications">
        <font>
            <Font size="18.0" />
        </font>
    </Button>
    <Button fx:id="backButton" layoutX="168.0" layoutY="421.0" mnemonicParsing="false" onAction="#onBackButton" prefHeight="26.0" prefWidth="60.0" text="Back" />
    <Label layoutX="273.0" layoutY="14.0" text="User Settings">
        <font>
            <Font size="30.0" />
        </font>
    </Label>
    <Label fx:id="errorMessageLabel" layoutX="235.0" layoutY="395.0" text="errorMessage" textFill="RED" visible="false" />
</AnchorPane>
